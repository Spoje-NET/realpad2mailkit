{
    "name": "spojenet/realpad2mailkit",
    "description": "Use mail adress from realpad in mailkit",
    "type": "project",
    "require": {
        "spojenet/realpad-takeout": "*",
        "meditorial/mailkit-api": "dev-DEVEDU-691-php-8-prepare"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.61",
        "ergebnis/composer-normalize": "^2.43",
        "ergebnis/php-cs-fixer-config": "^6.34"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Spojenet\\Realpad2mailkit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "vitezslav.dvorak@spojenet.cz"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
